{
  "name": "Github - Light Default",
  "dark": false,
  "author": "Yash Pal <iyashpal.dev@gmail.com>",
  "parentTheme": "IntelliJ",
  "editorScheme": "/theme/scheme/light-default.xml",
  "colors": {
    "white": "#ffffff",
    "black": "#0f172a",
    "foreground": "#1e293b",
    "background": "#ffffff",
    "focusColor": "#eae9e9",
    "borderColor": "#d0d7de",
    "focusedBorderColor": "#c1c9d0",
    "disabledForeground": "#1f232860",
    "selectionForeground": "#2f363d",
    "selectionBackground": "#ebf0f4",
    "defaultStartBackground": "#24c44d",
    "defaultEndBackground": "#10b43c",
    "defaultStartBorderColor": "#24c44d60",
    "defaultEndBorderColor": "#10b43c60"
  },
  "ui": {
    "*": {
      "foreground": "foreground",
      "background": "background",

      "focusColor": "focusColor",

      "borderColor": "borderColor",
      "focusedBorderColor": "focusedBorderColor",

      "selectionBackground": "selectionBackground",
      "selectionForeground": "selectionForeground",

      "selectionInactiveForeground": "selectionForeground",
      "selectionInactiveBackground": "selectionBackground"
    },

    "Button": {
      "foreground": "#25292e",
      "loadingForeground": "#25292e",
      "startBackground": "#ffffff",
      "endBackground": "#ffffff",
      "startBorderColor": "#d1d9e0",
      "endBorderColor": "#d1d9e0",
      "focusedBorderColor": "#d1d9e0",
      "default": {
        "foreground": "#ffffff",
        "loadingForeground": "#ffffff",
        "startBackground": "#1E883D",
        "endBackground": "#1E883D",
        "startBorderColor": "#1f232826",
        "endBorderColor": "#1f232826",
        "focusedBorderColor": "#1f232826",
        "focusColor": "#1f232826"
      }
    },

    "SegmentedButton": {
      "selectedStartBorderColor": "borderColor",
      "selectedEndBorderColor": "borderColor",
      "selectedButtonColor": "borderColor",
      "focusedSelectedButtonColor": "borderColor"
    },

    "SidePanel.background": "#f6f8fa",

    "MainToolbar.background": "#ffffff",
    "MainToolbar.separatorColor": "#d0d7de",

    "ToolBar.background": "white",

    "ScrollBar.background": "#ffffff00",
    "ScrollPane.background": "#ffffff00",



    "Popup.background": "#f6f8fa",

    "ToolWindow.background": "#ffffff",
    "ToolWindow.Header.background": "#f6f8fa",
    "ToolWindow.Header.inactiveBackground": "#f6f8fa",
    "ToolWindow.Header.borderColor": "borderColor",

    "TitlePane.background": "#f6f8fa",

    "TextPane.background": "#ffffff",
    "TextField.selectionBackground": "#3b82f6",
    "TextField.selectionForeground": "#ffffff",
    "TextArea.selectionBackground": "#3b82f6",
    "TextArea.selectionForeground": "#ffffff",

    "RunWidget": {
      "foreground": "foreground",
      "runningBackground": "#24c44d",
      "hoverBackground": "#00000019",
      "pressedBackground": "#00000028"
    },

    "ToolWindow": {
      "Button": {
        "selectedForeground": "#25292e",
        "selectedBackground": "#e6eaef",
        "hoverBackground": "#eff2f5"
      }
    }
  },
  "icons": {
    "ColorPalette": {
      "Tree.iconColor": "#334155",
      "Checkbox.Border.Default": "#475569",
      "Checkbox.Border.Disabled": "#cbd5e1",
      "Checkbox.Border.Selected": "#22c55e",
      "Checkbox.Background.Default": "#ffffff",
      "Checkbox.Background.Selected": "#22c55e",
      "Checkbox.Background.Disabled": "#e2e8f0",
      "Checkbox.Foreground.Selected": "#ffffff",
      "Checkbox.Foreground.Disabled": "#94a3b8",
      "Checkbox.Focus.Wide": "#6ee7b7",
      "Checkbox.Focus.Thin.Default": "#22c55e",
      "Checkbox.Focus.Thin.Selected": "#6ee7b7"
    }
  }
}