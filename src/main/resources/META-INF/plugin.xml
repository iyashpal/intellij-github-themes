<idea-plugin>
    <id>com.github.iyashpal.intellijgithubthemes</id>
    <name>GitHub Themes</name>
    <vendor email="iyashpal.dev@gmail.com" url="https://github.com/iyashpal">Yash Pal</vendor>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">

        <themeProvider id="39d9ff5d-0524-462f-bfd8-480f8bf8dcb2" path="/dark.theme.json"/>
        <themeProvider id="7b3f12e5-f633-44de-96e3-d37af87ddac6" path="/dark-dimmed.theme.json"/>
        <themeProvider id="9f345328-14d7-417f-bbbf-5e4e72aa9219" path="/dark-default.theme.json"/>
        <themeProvider id="e8ebbd6f-cff9-4a34-a71e-2383f53fdd70" path="/dark-colorblind.theme.json"/>
        <themeProvider id="724c8b26-41ed-437c-aea6-e944a7d197bb" path="/dark-high-contrast.theme.json"/>

        <themeProvider id="ab48fa2c-5781-4336-bcc6-550bb5e9cc7f" path="/light.theme.json"/>
        <themeProvider id="1c3fe902-f47b-47bf-9431-c3063efde008" path="/light-default.theme.json"/>
        <themeProvider id="c3988671-b69c-440b-819c-950627dbb0d9" path="/light-colorblind.theme.json"/>
        <themeProvider id="503f29fb-4e4a-44d4-b51b-92250ebfaa1d" path="/light-high-contrast.theme.json"/>

    </extensions>

    <change-notes>
        <![CDATA[
          <b>Change notes can be found https://github.com/iyashpal/intellij-github-themes/blob/main/CHANGELOG.md here.</b>
        ]]>
    </change-notes>
</idea-plugin>
